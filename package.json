    "lint:fmt": "npm run lint && npm run format"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.7",
    "inquirer": "^9.2.16",
    "midi-file": "^1.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.27",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  }
}
